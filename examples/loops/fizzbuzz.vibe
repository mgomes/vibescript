def fizzbuzz(limit)
  output = ""
  for n in 1..limit
    if n % 15 == 0
      output = output + "FizzBuzz\n"
    elsif n % 3 == 0
      output = output + "Fizz\n"
    elsif n % 5 == 0
      output = output + "Buzz\n"
    else
      output = output + n
      output = output + "\n"
    end
  end
  output
end
