def run
  h = { a: 1, b: 2, c: 3 }

  missing = h[:nonexistent]

  nested = { outer: { inner: { deep: 42 } } }
  deep_val = nested[:outer][:inner][:deep]

  empty = {}
  empty_keys = empty.keys
  empty_vals = empty.values

  merged = { x: 1 }.merge({ y: 2, x: 10 })

  with_nil = { present: 1, missing: nil }
  nil_val = with_nil[:missing]
  present_val = with_nil[:present]

  keys_count = h.keys.size
  vals_count = h.values.size

  {
    missing: missing,
    deep: deep_val,
    empty_keys: empty_keys,
    empty_vals: empty_vals,
    merged_x: merged[:x],
    merged_y: merged[:y],
    nil_val: nil_val,
    present_val: present_val,
    keys_count: keys_count,
    vals_count: vals_count
  }
end
